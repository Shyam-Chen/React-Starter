{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A boilerplate for React, Material, Babel, Flow, and ReactiveX.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=app webpack-dev-server --progress",
    "build": "rimraf public && cross-env BABEL_ENV=app webpack --env.prod",
    "lint": "eslint 'src/**/*.js'",
    "unit": "cross-env BABEL_ENV=app JEST_ENV=app jest \"src/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env BABEL_ENV=test JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
    "typed": "flow-typed install",
    "_codecov": "codecov",
    "_firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/React-Play.git"
