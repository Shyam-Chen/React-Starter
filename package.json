{
  "name": "React-Fullstack-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for React, Material, Express, Babel, Flow, and ReactiveX.",
  "scripts": {
    "start:app": "cross-env BABEL_ENV=app webpack-dev-server --progress --bail",
    "start:api": "cross-env BABEL_ENV=api nodemon src/server.js --exec babel-node",
    "build:app": "cross-env BABEL_ENV=app webpack --env.prod --progress --bail",
    "build:api": "cross-env BABEL_ENV=api babel src -d dist --ignore client.js,polyfill.js,app",
    "test:app": "cross-env BABEL_ENV=app jest \"src/app/.*\\.js$\" --coverage --forceExit",
    "test:api": "cross-env BABEL_ENV=api jest \"src/api/.*\\.js$\" --coverage --forceExit",
    "typed": "flow-typed install",
    "codecov": "codecov",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/React-Fullstack-Starter.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/React-Fullstack-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/React-Fullstack-Starter#readme",
